.who-am-i {\r\n    margin-top: 5rem;\r\n    display: grid;\r\n    grid-template-columns: 1fr 1fr;\r\n\r\n    .col {\r\n        .inner-heading-wrapper {\r\n            h3 {\r\n                font-size: 1.5rem;\r\n            }\r\n            p {\r\n                font-size: 1.2rem;\r\n                color: $blue;\r\n                padding-bottom: 0.5rem;\r\n            }\r\n        }\r\n\r\n        .content {\r\n            padding-top: 1rem;\r\n            color: $light-font;\r\n            p:not(:first-child) {\r\n                padding-top: 1rem;\r\n            }\r\n        }\r\n\r\n        .buttons {\r\n            padding-top: 1rem;\r\n            display: flex;\r\n            gap: 1rem;\r\n\r\n            // button:nth-child(2) {\r\n            //     background: #fff;\r\n            //     color: $light-font;\r\n            //     border: 1px solid $light-font;\r\n            // }\r\n\r\n            a {\r\n                .line {\r\n                    background: #fff;\r\n                    color: $light-font;\r\n                    border: 1px solid $light-font;\r\n                }\r\n                button {\r\n                    background: $blue;\r\n                    color: #fff;\r\n                    padding: 0.5rem 1rem;\r\n                    border-radius: 8rem;\r\n                }\r\n            }\r\n        }\r\n    }\r\n}\r\n\r\n.skills {\r\n    margin-top: 5rem;\r\n    .heading-wrapper {\r\n        @extend %heading;\r\n    }\r\n\r\n    .skill-boxes {\r\n        display: grid;\r\n        grid-template-columns: repeat(3, 1fr);\r\n        column-gap: 1rem;\r\n        row-gap: 1rem;\r\n\r\n        .skill-box-item {\r\n            cursor: pointer;\r\n            border: 1px solid $light-font;\r\n            text-align: center;\r\n            padding: 3rem 3rem;\r\n            transition: 100ms ease;\r\n\r\n            h3 {\r\n                padding-bottom: 1rem;\r\n                font-size: 1rem;\r\n            }\r\n\r\n            p {\r\n                color: $light-font;\r\n            }\r\n\r\n            &:hover {\r\n                background: $blue;\r\n                color: white;\r\n                p {\r\n                    color: #fff;\r\n                }\r\n                border-color: $blue;\r\n            }\r\n        }\r\n    }\r\n}\r\n\r\n.projects {\r\n    margin-top: 5rem;\r\n    .heading-wrapper {\r\n        @extend %heading;\r\n    }\r\n    \r\n    .project-boxes {\r\n        display: grid;\r\n        grid-template-columns: repeat(3, 1fr);\r\n        grid-gap: 1rem;\r\n\r\n        .project-box-item {\r\n            transition: 100ms ease;\r\n            cursor: pointer;\r\n            background: #222;\r\n            color: rgb(236, 236, 236);\r\n            border: 1px solid $light-font;\r\n            padding: 2rem 2rem;\r\n            h3 {\r\n                font-size: 1.2rem;\r\n                padding-bottom: 1rem;\r\n            }\r\n\r\n            p {\r\n                padding-bottom: 1rem;\r\n            }\r\n\r\n            &:hover {\r\n                background: $blue;\r\n                border-color: $blue;\r\n            }\r\n        }\r\n\r\n        button {\r\n            padding: 0.5rem 1rem;\r\n            background: #fff;\r\n            &:hover {\r\n                background: rgb(241, 241, 241);\r\n            }\r\n        }\r\n    }\r\n}\r\n\r\n.contact {\r\n    margin-top: 5rem;\r\n    padding-bottom: 5rem;\r\n\r\n    a {\r\n        background: #CD3C30;\r\n        text-decoration: none;\r\n        color: #fff;\r\n        padding: 0.5rem 1rem;\r\n        border-radius: 1rem;\r\n    }\r\n    .heading-wrapper {\r\n        @extend %heading;\r\n    }\r\n}\r\n\r\n@media(max-width: 1140px) {\r\n    .container {\r\n        padding: 0 1rem;\r\n    }\r\n    .who-am-i {\r\n        text-align: center;\r\n        grid-template-columns: 1fr;\r\n\r\n        .col {\r\n            .inner-heading-wrapper {\r\n                padding-top: 1rem;\r\n            }\r\n\r\n            justify-self: center;\r\n        }\r\n\r\n        .buttons {\r\n            justify-content: center;\r\n        }\r\n    }\r\n\r\n    .skills {\r\n        .skill-boxes {\r\n            grid-template-columns: 1fr 1fr;\r\n        }\r\n    }\r\n}\r\n\r\n@media(max-width: 900px) {\r\n    img {\r\n        width: 100%;\r\n    }\r\n    .projects {\r\n        .project-boxes {\r\n            grid-template-columns: 1fr 1fr;\r\n        }\r\n    }\r\n\r\n    .contact {\r\n        padding-bottom: 5rem;\r\n    }\r\n}\r\n\r\n@media(max-width: 600px) {\r\n    .skills {\r\n        .skill-boxes {\r\n            grid-template-columns: 1fr;\r\n        }\r\n    }\r\n\r\n    .projects {\r\n        .project-boxes {\r\n            grid-template-columns: 1fr;\r\n        }\r\n    }\r\n\r\n    .contact {\r\n        padding-bottom: 5rem;\r\n    }\r\n}"},"lineCount":null}},"error":null,"hash":"b78ae84f514d3a88a80e8d8bd929850c","cacheData":{"env":{}}}